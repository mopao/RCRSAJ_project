PRAGMA foreign_keys=OFF;
BEGIN TRANSACTION;
CREATE TABLE ABUS(
Buveur TEXT PRIMARY KEY NOT NULL,
Cru CHAR(25) not NULL,
Millesime INT NOT NULL);
INSERT INTO "ABUS" VALUES('Pierre','Chablis',2008);
INSERT INTO "ABUS" VALUES('Pierre','Bordeaux',1992);
INSERT INTO "ABUS" VALUES('Jean','Julienas',1997);
CREATE TABLE VINS(
Cru CHAR(25) NOT NULL,
Millesime INT NOT NULL,
Qualite CHAR NOT NULL,
PRIMARY KEY(Cru,Millesime,Qualite));
INSERT INTO "VINS" VALUES('Chablis',2008,'A');
INSERT INTO "VINS" VALUES('Chablis',2008,'B');
INSERT INTO "VINS" VALUES('Julienas',1997,'C');
INSERT INTO "VINS" VALUES('Bordeaux',1992,'A');
